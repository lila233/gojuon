{
  "expo": {
    "name": "gojuon-app",
    "slug": "gojuon-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/audio/*"
    ],
    "plugins": [
      "expo-av"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lila336.gojuonapp"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "static",
      "baseUrl": "/gojuon"
    },
    "extra": {
      "eas": {
        "projectId": "e65066bc-c808-478f-8fac-2170e1989a20"
      }
    }
  }
}
